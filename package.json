{
  "$schema": "http://json.schemastore.org/package",
  "name": "wcf-date",
  "version": "1.0.5",
  "description": "Utilities for converting between date objects and WCF date representation strings",
  "main": "dist/wcf-date.js",
  "scripts": {
    "clean": "node \"build-scripts/clean\"",
    "copydts": "node \"build-scripts/copydts\"",
    "build:common": "echo Transpiling TypeScript to ES2015... && tsc && echo Transpiling ES2015 to ES5... && babel es2015 -d dist",
    "build": "npm run clean && npm run build:common && npm run copydts",
    "prepublish": "npm run build",
    "test": "jasmine"
  },
  "files": [
    "wcf-date.ts",
    "dist",
    "es2015"
  ],
  "keywords": [
    "WCF",
    "date",
    "conversion",
    "parse"
  ],
  "directories": {
    "test": "spec"
  },
  "author": "WSDOT",
  "contributors": [
    "Jeff Jacobson (https://github.com/JeffJacobson/)"
  ],
  "jsnext:main": "es2015/wcf-date.js",
  "license": "Unlicense",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.13.2",
    "eslint": "^3.3.1",
    "eslint-config-defaults": "^9.0.0",
    "eslint-config-standard": "^5.3.5",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.1.2",
    "eslint-plugin-standard": "^2.0.0",
    "fs-extra": "^0.30.0",
    "jasmine": "^2.4.1",
    "typescript": "~1.8.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WSDOT-GIS/wcf-date.git"
  },
  "bugs": {
    "url": "https://github.com/WSDOT-GIS/wcf-date/issues"
  },
  "homepage": "https://github.com/WSDOT-GIS/wcf-date#readme"
}
